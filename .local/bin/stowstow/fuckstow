#!/bin/bash

stowed="bin bspwm calcurse fonts git gnupg gtk"
stowed="$stowed homeconfig homedir irssi media"
stowed="$stowed neovim newsboat notification"
stowed="$stowed polybar pulse share sxhkd sxiv"
stowed="$stowed transmission vifm wget xorg"
stowed="$stowed zathura zsh"

cd ~/.dots/stow_home
stow -Dt ~ $stowed
